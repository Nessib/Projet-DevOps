pipeline {
    agent any

    stages {
        stage('Préparation') {
            steps {
                sh 'git clone https://github.com/Nessib/Projet-DevOps.git'
                // Autres commandes nécessaires...
            }
        }

        stage('Virtualisation et Déploiement Docker') {
            steps {
                sh 'docker --version'
                sh 'docker-compose up -d'
                // Autres commandes Docker nécessaires...
            }
        }
    }
}
